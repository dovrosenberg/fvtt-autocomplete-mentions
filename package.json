{
  "name": "fvtt-autocomplete-mentions",
  "title": "Foundry VTT Autocomplete Mentions",
  "version": "2.0.0", 
  "description": "Easily reference actors, scenes, items, etc. in the editor.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dovrosenberg/fvtt-autocomplete-mentions.git"
  },
  "author": "Dov Rosenberg",
  "homepage": "https://github.com/dovrosenberg/fvtt-autocomplete-mentions#readme",
  "main": "main.ts",
  "scripts": {
    "build": "vite build",
    "debug": "vite build --minify=false --mode development",
    "copy": "rm -rf ~/foundry13data/Data/modules/autocomplete-mentions/* && cp -r $(pwd)/dist/* ~/foundryData/Data/modules/autocomplete-mentions",
    "linkdata": "ln -s $PWD/dist ~/foundry13data/Data/modules/autocomplete-mentions",
    "tsc": "tsc --noEmit --skipLibCheck",
    "unlinkdata": "unlink ~/foundry13data/Data/modules/autocomplete-mentions",
    "buildcopy": "npm run build && npm run copy",
    "foundry": "node ~/foundry13/resources/app/main.js"
  },
  "devDependencies": {
    "@league-of-foundry-developers/foundry-vtt-types": "9.280.0",
    "@originjs/vite-plugin-commonjs": "^1.0.1",
    "@types/marked": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "css-loader": "^6.4.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "marked": "^3.0.7",
    "mini-css-extract-plugin": "^2.4.2",
    "node-sass": "^9.0.0",
    "node-sass-glob-importer": "^5.3.2",
    "null-loader": "^4.0.1",
    "prettier": "^2.4.1",
    "raw-loader": "^4.0.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-scss": "^4.0.0",
    "sass": "^1.45.0",
    "sass-loader": "^13.3.2",
    "semver-increment": "^1.0.1",
    "source-map-loader": "^3.0.0",
    "string-replace-loader": "^3.0.3",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.2.6",
    "vite": "^2.7.2",
    "vite-plugin-env-compatible": "^1.1.1",
    "vite-plugin-html": "3.2.0"
  }
}
